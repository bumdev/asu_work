        <script>
            function enterToTab() {
                if (event.keyCode == 13)
                    event.keyCode = 9;
            }
            $(document).ready(function () {
                $("input").bind("keydown", function (event) {
                    if (event.which === 13) {
                        event.stopPropagation();
                        event.preventDefault();
                        $(this).next("input").focus();
                    }
                });
            });
    </script>
